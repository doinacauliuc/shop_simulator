<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogInController">
   <children>
      <Text fill="#2e3680" layoutX="75.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Log In and Start Shopping" textAlignment="CENTER">
         <font>
            <Font name="Sama Kannada ExtraBold" size="36.0" />
         </font>
      </Text>
      <TextField fx:id="Username" layoutX="235.0" layoutY="124.0" prefHeight="27.0" prefWidth="229.0" promptText="Username">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Text fill="#2e3680" layoutX="142.0" layoutY="142.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text fill="#2e3680" layoutX="142.0" layoutY="195.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <PasswordField fx:id="Password" layoutX="235.0" layoutY="176.0" prefHeight="26.0" prefWidth="229.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Button layoutX="409.0" layoutY="223.0" mnemonicParsing="false" onAction="#logIn" text="Log In" textFill="#2e3680">
